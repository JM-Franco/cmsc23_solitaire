   <children>
      <VBox fx:id="highscoreDisplay" layoutX="59.0" layoutY="86.0" prefHeight="342.0" prefWidth="257.0" />
      <Label layoutX="104.0" layoutY="26.0" style="-fx-background-color: rgba(255, 255, 255, 0.5);; -fx-effect: dropshadow(gaussian, white, 30, 0, 0, 0);; -fx-background-insets: 100;" text="High Scores">
         <font>
